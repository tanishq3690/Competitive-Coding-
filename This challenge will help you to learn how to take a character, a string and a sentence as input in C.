#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{char ch, s[100],sen[100];

    scanf("%c\n",&ch);
    scanf("%s",&s);

    scanf("\n%[^\n]%*c",&sen);
    
    printf("%c\n%s\n",ch,s);
    printf("%s",sen);
    return 0;
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
